<Font encoding="UTF-8">#\345\207\272\345\212\233\357\274\232\343\203\252\343\202\271\343\203\210\345\244\232\351\240\205\345\274\217\343\201\253\345\200\244\343\202\222\344\273\243\345\205\245\343\201\227\343\201\237\351\232\233\343\201\253\345\276\227\343\202\211\343\202\214\343\202\213\345\200\244
</Font>substitute := proc(Pol,val);
local i,result,index,pol;

pol := change_0_to_zero(Pol);
result := G4:-zero;
for i from 1 by 1 to nops(pol) do
  result := G4:-`+`(result,G4:-`*`(pol[i],(G4:-`^`(val,(i-1)))));
end do;
return result;
end proc:</Text-field>
