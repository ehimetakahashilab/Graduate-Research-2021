  while dividend[nops(dividend)]=G4:-zero do
    dividend := subsop(i=NULL,dividend);
    i--;
<Font encoding="UTF-8">    #\345\211\262\343\202\211\343\202\214\343\202\213\346\225\260\343\201\256\346\234\200\344\270\212\344\275\215\343\201\214zero\343\201\256\346\231\202\343\200\201\345\225\206\343\201\253\344\270\200\343\201\244\343\201\240\343\201\221zero\343\202\222\345\212\240\343\201\210\343\202\213\343\201\237\343\202\201\343\201\256\346\223\215\344\275\234
</Font>    flag++;
    if flag=1 then
