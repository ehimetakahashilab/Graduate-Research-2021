#\345\207\272\345\212\233\357\274\232\345\205\245\345\212\233\343\201\227\343\201\237\345\244\232\351\240\205\345\274\217\343\203\252\343\202\271\343\203\210\343\201\256\345\211\262\343\202\212\347\256\227\343\201\256\347\265\220\346\236\234\343\201\256\345\225\206([1])\343\201\250\344\275\231\343\202\212([2])\343\202\222\350\241\250\343\201\231\344\272\214\351\207\215\345\244\232\351\240\205\345\274\217\343\203\252\343\202\271\343\203\210</Font>

division_on_G4 := proc(x,y);
local i,j,q,quotient,Top_divisor,divisor,dividend,flag;
local result:=[[],[]];

dividend := x;
divisor := y;
q:=0;

<Font encoding="UTF-8">#\345\211\262\343\202\213\343\203\252\343\202\271\343\203\210\345\211\262\343\202\211\343\202\214\343\202\213\343\203\252\343\202\271\343\203\210\343\201\256\344\275\231\345\210\206\343\201\2520\343\202\222\344\270\212\343\201\213\343\202\211\351\231\244\343\201\217
</Font>i := nops(dividend);
while dividend[nops(dividend)]=0 do
  dividend := subsop(i=NULL,dividend);
  i--;
