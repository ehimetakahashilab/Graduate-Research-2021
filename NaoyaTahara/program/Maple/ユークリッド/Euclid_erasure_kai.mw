#\345\207\272\345\212\233\357\274\232p*q\343\201\256\344\271\227\347\256\227\347\265\220\346\236\234r</Font>
mul_on_g := proc(g, p, q)
  local i, j, m, n, r;
  m := numelems(p); n := numelems(q);
  r := Vector(m+n-1, fill = g:-zero);
  for i from 1 to m do
    for j from 1 to n do
      r[i+j-1] := g:-`+`(r[i+j-1], g:-`*`(p[i], q[j]));
    end do;
  end do;
  return r;
end proc:</Text-field>
