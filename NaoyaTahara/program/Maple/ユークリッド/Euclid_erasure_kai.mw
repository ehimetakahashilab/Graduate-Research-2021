<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font encoding="UTF-8">#\351\226\242\346\225\260\345\220\215\357\274\232div_on_g</Font>
<Font encoding="UTF-8">#\345\205\245\345\212\233\357\274\232\343\202\254\343\203\255\343\202\242\344\275\223g, \345\244\232\351\240\205\345\274\217\357\274\210\343\203\231\343\202\257\343\203\210\343\203\253\350\241\250\347\217\276\357\274\211u, </Font>v
<Font encoding="UTF-8">#\345\207\272\345\212\233\357\274\232u/v\343\201\256\345\225\206q,\345\211\260\344\275\231r</Font>
div_on_g := proc(g, u, v)
  local i, j, m, n, q, r;
  m := numelems(u); n := numelems(v);
  r := Vector(m, u);
  if (m &lt; n) then
    return g:-zero, r;
  end if;
  q := Vector(m - n + 1, fill = g:-zero);
  for i from m - n + 1 by -1 to 1 do
    q[i] := g:-`/`(r[n + i - 1], v[n]);
    for j from n by -1 to 1 do
      r[i + j - 1] := g:-`-`(r[i + j - 1], g:-`*`(q[i], v[j]));
    end do;
  end do;
  for i from m by -1 to 1 do
    if (r[i] = g:-zero) then
      m := m - 1;
    else
      break;
    end if;
  end do;
  r := Vector(m, convert(r, list));
  return q, r;
end proc:</Text-field>
</Input>
</Group>
