#\345\207\272\345\212\233\357\274\232\345\205\245\345\212\233\343\201\227\343\201\237\345\244\232\351\240\205\345\274\217\343\203\252\343\202\271\343\203\210\345\206\205\343\201\256G4\344\270\212\343\201\256zero\343\202\2220\343\201\253\345\244\211\346\217\233</Font>
change_zero_to_0 := proc(x);
local i,X;
X := x;

for i from 1 by 1 to nops(x) do
  if x[i] = G4:-zero then
