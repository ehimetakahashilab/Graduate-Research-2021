<Group hide-input="false" labelreference="L429" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font encoding="UTF-8">#\345\276\251\345\217\267\343\202\222\350\241\214\343\201\206
</Font>v := EX_bin;
for i from 1 by 1 to nops(erasure_location) do
  EL := erasure_location[i];
  dividend := substitute(Z,G4:-`^`(a,-EL));
  divisor := substitute(diff_Gamma,G4:-`^`(a,-EL));
  v[op(erasure_location[i]+1)] := G4:-`/`(dividend,divisor);
end do;
for i from 1 by 1 to nops(error_location) do
  EL := error_location[i];
  dividend := substitute(Z,G4:-`^`(a,-EL));
  divisor := substitute(diff_Gamma,G4:-`^`(a,-EL));
  E := G4:-`/`(dividend,divisor);
  v[op(error_location[i]+1)] := G4:-`+`(v[op(error_location[i]+1)],E);
end do;
change_zero_to_0(v);</Text-field>
</Input>
