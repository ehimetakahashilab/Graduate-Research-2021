<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font encoding="UTF-8">#\351\226\242\346\225\260\345\220\215\357\274\232get_exp
#\345\205\245\345\212\2331\357\274\2322\343\201\2568\344\271\227\343\201\256\343\202\254\343\203\255\343\202\242\344\275\223\344\270\212\343\201\256\345\244\232\351\240\205\345\274\217\350\241\250\347\217\276
#\345\207\272\345\212\233\357\274\232\345\244\232\351\240\205\345\274\217\350\241\250\347\217\276\343\201\256\345\205\203\343\201\250\343\201\252\343\202\213\343\202\242\343\203\253\343\203\225\343\202\241\343\201\256\345\206\252</Font>

get_exp := proc(dec):
local exp, fin, pol,G8,a;
G8 := GF(2, 8, alpha^8+alpha^4+alpha^3+alpha^2+1):
a := G8:-ConvertIn(alpha):
exp := 0;
pol := G8:-input(dec);
fin := G8:-`^`(a,0);
if dec = 0 then
  exp := -1;
else
  while pol&lt;&gt;fin do
    pol := G8:-`/`(pol,a);
    exp := exp+1;
  end do:
end if;
return exp;
end proc:</Text-field>
</Input>
